{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      { "src": "/api(.*)", "dest": "index.js" },
      { "src": "/(.*)", "dest": "/src/app/page.js" }
      ]
  }
  